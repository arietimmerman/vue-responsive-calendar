{
  "name": "vue-responsive-calendar",
  "description": "A responsive calendar with Vue",
  "version": "1.0.7",
  "author": "Arie Timmerman",
  "private": false,
  "main": "dist/calendar.min.js",
  "jsnext:main": "src/index.js",  
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
